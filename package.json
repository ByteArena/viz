{
  "name": "bytearena-viz",
  "version": "0.0.1",
  "description": "Byte Arena SDK",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/",
    "build:js": "rollup -c rollup.config.js",
    "build": "make build",
    "_build": "npm run --silent lint && npm run --silent build:js",
    "install-and-build": "npm install && npm run --silent lint && npm run --silent build:js",
    "_watch:dev": "chokidar 'src/**/*.{js,ts}' -c 'npm run --silent build'",
    "_watch:dev:nolint": "chokidar 'src/**/*.{js,ts}' -c 'npm run --silent build:js'",
    "_livereload": "live-server --no-browser --cors --watch=./lib",
    "dev": "npm run build && parallelshell 'npm run _watch:dev' 'npm run _livereload'",
    "dev:nolint": "npm run build && npm run _watch:dev:nolint",
    "clean": "rm -Rf lib/*",
    "prepublish": "npm run build"
  },
  "license": "MIT",
  "homepage": "https://bytearena.com",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.32",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.0.0-beta.32",
    "@babel/preset-flow": "^7.0.0-beta.32",
    "@babel/preset-react": "^7.0.0-beta.32",
    "@babel/cli": "^7.0.0-beta.32",
    "babel-eslint": "8",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-emotion": "^8.0.10",
    "babel-preset-babili": "0.1.4",
    "babel-preset-es2015": "^6.24.1",
    "bytearena-sdk-vector2": "^0.0.14",
    "charcodes": "^0.0.10",
    "chokidar-cli": "^1.2.0",
    "css-loader": "^0.28.7",
    "emotion": "^8.0.10",
    "eslint": "^4.11.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-flowtype-errors": "^3.3.6",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "flow-bin": "^0.59.0",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "live-server": "^1.2.0",
    "parallelshell": "^3.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-emotion": "^8.0.10",
    "react-rangeslider": "^2.2.0",
    "react-split-pane": "^0.1.71",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-babili": "^3.0.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^4.3.1",
    "style-loader": "^0.19.0",
    "webpack": "3.0.0"
  },
  "dependencies": {
    "bytearena-sdk": "0.0.1",
    "denque": "^1.1.1"
  }
}
